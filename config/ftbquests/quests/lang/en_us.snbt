	quest.7844AE79F1E9199A.quest_subtitle: "Visit the biome Jungle."
	quest.78D955BE9717DBE3.quest_desc: ["Till soil, plant wheat, hydrate, and harvest."]
	quest.78D955BE9717DBE3.quest_subtitle: "Bread won’t bake itself."
	quest.7FA47A43E76F2EEF.quest_desc: ["Endless plains of purple lavender flowers with a calming scent."]
	quest.7FA47A43E76F2EEF.quest_subtitle: "Visit the biome Lavender Fields"
}
