	quest.47F2A3F359713DB1.quest_desc: ["Start charting your surroundings for navigation."]
	quest.47F2A3F359713DB1.quest_subtitle: "Know your land, draw it down."
	quest.47F8FAB897A6F452.quest_desc: ["Temperate woods with oak and birch, flowers and bees; easy wood access and common mobs."]
	quest.47F8FAB897A6F452.quest_subtitle: "Visit the biome Forest."
	quest.481610F436A06707.quest_desc: ["Turn raw iron into ingots—your gateway to stronger tools, armor, and utilities."]
	quest.481610F436A06707.quest_subtitle: "Smelt the basics."
	quest.4AB92A705803846C.quest_desc: ["Upgrade to stone for speed and durability."]
	quest.4AB92A705803846C.quest_subtitle: "Stronger tools, stronger you."
	quest.4C614AE2FE22D3A7.quest_desc: ["The best way to travel around the world"]
	quest.4C614AE2FE22D3A7.quest_subtitle: "Fly faster with elytra"
